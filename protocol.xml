					<Position>
						<Page>General</Page>
						<Row>4</Row>
						<Column>0</Column>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type>string</Type>
			</Measurement>
		</Param>
	</Params>
	
	<QActions>
	</QActions>
	
	<Groups>
